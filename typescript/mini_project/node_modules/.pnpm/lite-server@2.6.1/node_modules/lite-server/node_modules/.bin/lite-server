#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/giovanirubini/Documents/learning/code/typescript/mini_project/node_modules/.pnpm/lite-server@2.6.1/node_modules/lite-server/bin/node_modules:/Users/giovanirubini/Documents/learning/code/typescript/mini_project/node_modules/.pnpm/lite-server@2.6.1/node_modules/lite-server/node_modules:/Users/giovanirubini/Documents/learning/code/typescript/mini_project/node_modules/.pnpm/lite-server@2.6.1/node_modules:/Users/giovanirubini/Documents/learning/code/typescript/mini_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/giovanirubini/Documents/learning/code/typescript/mini_project/node_modules/.pnpm/lite-server@2.6.1/node_modules/lite-server/bin/node_modules:/Users/giovanirubini/Documents/learning/code/typescript/mini_project/node_modules/.pnpm/lite-server@2.6.1/node_modules/lite-server/node_modules:/Users/giovanirubini/Documents/learning/code/typescript/mini_project/node_modules/.pnpm/lite-server@2.6.1/node_modules:/Users/giovanirubini/Documents/learning/code/typescript/mini_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/lite-server" "$@"
else
  exec node  "$basedir/../../bin/lite-server" "$@"
fi
